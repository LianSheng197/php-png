RewriteEngine On
RewriteBase /

# redirect to source code
RewriteRule ^(.+)\/code$ https://github.com/LianSheng197/php-png [L,R=307]

# https
RewriteCond %{HTTP:X-Forwarded-Proto} https 
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.+)$ index.php?s=$1 [L,QSA]

# http
RewriteCond %{HTTP:X-Forwarded-Proto} !https 
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.+)$ index.php?s=$1 [L,QSA]